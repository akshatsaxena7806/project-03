<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.co.rays.project_3.dto">

	<class name="InventoryDTO" table="ST_INVENTORY">

		<id name="id" type="java.lang.Long">
			<column name="ID"/>
			<generator class="increment"/>
		</id>

		<property name="supplierName" type="java.lang.String">
			<column name="SUPPLIER_NAME"/>
		</property>

		<property name="dob" type="java.util.Date">
			<column name="DOB"/>
		</property>

		<property name="quantity" type="java.lang.Long">
			<column name="QUANTITY"/>
		</property>

		<property name="product" type="java.lang.String">
			<column name="PRODUCT"/>
		</property>

		

		

	</class>

</hibernate-mapping>

